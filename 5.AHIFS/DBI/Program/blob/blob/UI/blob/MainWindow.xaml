<Window x:Class="blob.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:blob"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btn_load_file" Content="Button" HorizontalAlignment="Left" Margin="10,258,0,0" VerticalAlignment="Top" Width="498" Height="53" Click="btn_load_file_Click"/>
        <Button x:Name="btn_get_blobs" Content="Get Blobs" HorizontalAlignment="Left" VerticalAlignment="Top" Width="498" Height="22" Click="btn_get_blobs_Click" Margin="10,10,0,0"/>
        <ListView x:Name="list_blobs" HorizontalAlignment="Left" Height="197" Margin="10,37,0,0" VerticalAlignment="Top" Width="498" SelectionChanged="list_blobs_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
