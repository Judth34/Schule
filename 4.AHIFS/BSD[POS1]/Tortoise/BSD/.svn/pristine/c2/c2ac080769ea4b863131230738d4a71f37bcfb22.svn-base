    "version": "0.2.0",
    "compounds": [
        {
            "name": "Launch & Debug Backend and Frontend",
            "configurations": ["Start Backend", "Start Chrome"]
        }
    ],
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Start Chrome",
            "url": "http://localhost:8080/api",
            "webRoot": "${workspaceFolder}"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Start Backend",
            "program": "${workspaceFolder}\\Backend\\sessionRaterMain.js"
        }
    ]
}